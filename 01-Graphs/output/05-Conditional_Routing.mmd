---
config:
  flowchart:
    curve: linear
---
graph TD;
	__start__([<p>__start__</p>]):::first
	square_node(square_node)
	abs_node(abs_node)
	zero_node(zero_node)
	router(router)
	__end__([<p>__end__</p>]):::last
	__start__ --> router;
	router -. &nbsp;negative_branch&nbsp; .-> abs_node;
	router -. &nbsp;positive_branch&nbsp; .-> square_node;
	router -. &nbsp;zero_branch&nbsp; .-> zero_node;
	abs_node --> __end__;
	square_node --> __end__;
	zero_node --> __end__;
	classDef default fill:#f2f0ff,line-height:1.2
	classDef first fill-opacity:0
	classDef last fill:#bfb6fc
	
