        "-v",
        "-s",
        "./01-Graphs",
        "-p",
        "test_*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
    "claudeCodeChat.permissions.yoloMode": true,
    "DockerRun.DisableAutoGenerateConfig": true,
    "hide-files.files": [
      ".ruff_cache",
      ".virtual_documents",
      "GEMINI.md",
      "Makefile",
      "LICENSE",
      "langgraph.json",
      "context_portal",
      ".aiignore"
    ]
}