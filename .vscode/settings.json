{
    "python-envs.defaultEnvManager": "ms-python.python:conda",
    "python-envs.defaultPackageManager": "ms-python.python:conda",
    "python-envs.pythonProjects": [],
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/.DS_Store": true,
      "**/Thumbs.db": true,
      ".idea": true,
      ".vscode": true,
      "**/__pycache__": true,
      "**/.jupytext-sync-ipynb": true,
      "**/.roo": true,
      ".roo": true,
      "**/.zed": true,
      ".zed": true,
      "**/.mypy_cache": true,
      ".mypy_cache": true,
      "**/.junie": true,
      ".junie": true,
      "**/.continue": true,
      ".continue": true,
      "**/.github": true,
      "**/.kilocode": true,
      ".kilocode": true,
      "**/.pytest_cache": true,
      ".ruff_cache": true,
      ".virtual_documents": true,
      "GEMINI.md": true,
      "Makefile": true,
      "LICENSE": true,
      "langgraph.json": true,
      "context_portal": true,
      ".aiignore": true
    },
    "explorerExclude.backup": {},
